
<h2>Доступные версии пакета</h2>

<div class="ui relaxed list">
    @foreach(var row in Model) {
        <div class="item">
            <i class="large download middle aligned icon"></i>
            <div class="content">
                <a class="header" href="@row.ФайлВерсии">@row.НомерВерсии</a>
                <div class="description">Изменен: @row.ДатаВерсии</div>
            </div>
        </div>
    }
</div>