<h1>Пакеты</h1>

<table class="ui celled table">
    <thead>
        <tr>
            <th>Название</th>
            <th>Актуальная версия</th>
            <th>Адрес репозитория</th>
        </tr>
    </thead>
    <tbody>
    
    @if(@Model != null) {
        @foreach(var pack in @Model) {
            <tr>
                <td><a asp-action="Details" asp-route-id="@pack.id">@pack.id</a></td>
                <td>@pack.lastVersion</td>
                <td>&lt;нет данных&gt;</td>
            </tr>
        }
    }
    </tbody>
    
</table>